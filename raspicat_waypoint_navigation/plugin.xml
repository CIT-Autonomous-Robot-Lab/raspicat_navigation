<library path="lib/libraspicat_waypoint_navigation">
  <class
    name="raspicat_waypoint_navigation/WaypointServer"
    type="raspicat_navigation::WaypointServer"
    base_class_type="raspicat_navigation::BaseWaypointServer">
    <description>
      Plugins for managing waypoints.
    </description>
  </class>

  <class
    name="raspicat_waypoint_navigation/WaypointRviz"
    type="raspicat_navigation::WaypointRviz"
    base_class_type="raspicat_navigation::BaseWaypointRviz">
    <description>
      Plugins for drawing in rviz with respect to waypoint.
    </description>
  </class>

  <class
    name="raspicat_waypoint_navigation/ParamChange"
    type="raspicat_navigation::ParamChange"
    base_class_type="raspicat_navigation::WaypointNavHelperPlugin">
    <description>
      Plugin to change dynamic parameters related to waypoint.
    </description>
  </class>

  <class
    name="raspicat_waypoint_navigation/SlopeObstacleAvoidance"
    type="raspicat_navigation::SlopeObstacleAvoidance"
    base_class_type="raspicat_navigation::WaypointNavHelperPlugin">
    <description>
      Plugin to stable navigation on slopes.
    </description>
  </class>

  <class
    name="raspicat_waypoint_navigation/ClearCostMap"
    type="raspicat_navigation::ClearCostMap"
    base_class_type="raspicat_navigation::WaypointNavHelperPlugin">
    <description>
      Plugin to run clear cost map for each waypoint.
    </description>
  </class>

  <class
    name="raspicat_waypoint_navigation/WaitingLine"
    type="raspicat_navigation::WaitingLine"
    base_class_type="raspicat_navigation::WaypointNavHelperPlugin">
    <description>
      Plugin to waiting line.
    </description>
  </class>
</library>